---
layout: null
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
  <title>{{ site.me.name }}</title>
  <language>en-US</language>
  <link href="{{ site.url }}/atom.xml" rel="self"/>
  <link href="{{ site.url }}/"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <description>{{ site.description }}</description>
  <id>{{ site.url }}/</id>
  <author>
    <name>{{ site.me.name }}</name>
    <email>{{ site.me.email }}</email>
  </author>
  {% for post in site.categories.photo limit:10 %}
  <entry>
    <title>{{ post.title | xml_escape }}</title>
    <link href="{{ site.url }}{{ post.url }}"/>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <id>{{ site.url }}{{ post.id }}</id>
    <content type="html">
      &lt;img alt=&quot;{{photo.title | xml_escape}}&quot; src=&quot;{{site.photo_prefix}}{{post.photo}}_2880.jpg&quot;/&gt;
      {{ post.content | xml_escape }}
    </content>
    <author>
      <name>{{ site.me.name }}</name>
      <uri>{{ site.url }}/about/</uri>
    </author>
  </entry>
  {% endfor %}
</feed>
